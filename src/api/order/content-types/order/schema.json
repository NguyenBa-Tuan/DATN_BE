{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "totalPrice": {
      "type": "decimal"
    },
    "address": {
      "type": "string"
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "cash",
        "card"
      ],
      "default": "cash"
    },
    "product": {
      "displayName": "listProducts",
      "type": "component",
      "repeatable": true,
      "component": "product.list-products"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "new",
        "delivering",
        "paid",
        "refuse"
      ]
    },
    "listProducts": {
      "type": "json"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    }
  }
}
